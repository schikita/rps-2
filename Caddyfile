rps-game.ru {
    route {
        reverse_proxy /api* backend:3000
        reverse_proxy /auth* backend:3000
        reverse_proxy /health* backend:3000
        reverse_proxy * cyber-rps:80
    }

    log {
        output file /var/log/caddy/access.log
    }
}

:80 {
    reverse_proxy cyber-rps:80
}
